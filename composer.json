{
    "name": "adoreparler/seat-charinfo",
    "description": "A SeAT plugin for viewing character info summary",
    "type": "seat-plugin",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illidan/seat-services": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Seat\\Charinfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seat\\Charinfo\\CharinfoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
